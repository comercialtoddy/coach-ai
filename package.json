{
  "name": "coach-ai",
  "version": "1.0.0",
  "description": "CS2 Competitive Overlay with AI Coach Integration",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "cross-env NODE_ENV=development electron .",
    "dev-console": "cross-env NODE_ENV=development DEBUG=* electron .",
    "dev-win": "set NODE_ENV=development&& electron .",
    "build": "electron-builder",
    "test": "jest"
  },
  "keywords": [
    "cs2",
    "overlay",
    "gaming",
    "ai-coach",
    "electron"
  ],
  "author": "Coach AI Team",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.19.0",
    "axios": "^1.10.0",
    "dotenv": "^16.6.1",
    "node-cache": "^5.1.2",
    "screenshot-desktop": "^1.15.0",
    "sharp": "^0.33.0"
  },
  "build": {
    "appId": "com.coachai.cs2overlay",
    "productName": "CS2 Coach AI",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    }
  }
}
